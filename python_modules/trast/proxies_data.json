  "dynamic_proxies": {
    "description": "Прокси загружаются динамически из внешних источников",
    "storage_key": "servers",
    "storage_location": "chrome.storage.local",
    "sources": [
      {
        "type": "telegram",
        "url": "https://t.me/liservers/15"
      },
      {
        "type": "telegraph",
        "url": "https://telegra.ph/Servers-10-16-15"
      }
    ],
    "format": {
      "encoding": "base64",
      "structure": "host:port:login:pass|date|country|flags",
      "fields": [
        {
          "index": 0,
          "name": "proxy_info",
          "format": "host:port:login:pass",
          "description": "Основная информация о прокси"
        },
        {
          "index": 1,
          "name": "expiry_date",
          "format": "YYYY-MM-DD",
          "description": "Дата окончания действия"
        },
        {
          "index": 2,
          "name": "country_code",
          "format": "ISO 3166-1 alpha-2",
          "description": "Код страны прокси"
        },
        {
          "index": 3,
          "name": "flags",
          "format": "string",
          "description": "Дополнительные флаги (например: 'rmd' для случайного выбора)",
          "optional": true
        }
      ]
    },
    "processing": {
      "filter_banned_countries": true,
      "banned_countries_key": "bancountries",
      "random_selection": {
        "enabled": true,
        "flag": "rmd",
        "description": "Прокси с флагом |rmd могут быть выбраны случайным образом"
      },
      "priority": "high",
      "description": "Динамические прокси добавляются в начало списка serverConfigs"
    }
  },
  "vip_proxies": {
    "description": "Премиум прокси-серверы",
    "storage_key": "vipserver",
    "storage_location": "chrome.storage.local",
    "format": "json",
    "structure": {
      "host": "string",
      "port": "string",
      "user": "string",
      "pass": "string",
      "date_end": "string (YYYY-MM-DD HH:mm:ss)",
      "country": "string (optional, ISO 3166-1 alpha-2)"
    },
    "activation": {
      "methods": [
        {
          "type": "payment",
          "description": "Активация через оплату"
        },
        {
          "type": "vip_code",
          "format": "base64",
          "structure": "host:port:user:pass|date_end|country",
          "description": "Активация через VIP код"
        }
      ]
    },
    "features": {
      "priority": "highest",
      "expiry_check": true,
      "auto_fallback": true,
      "description": "При истечении срока автоматически переключается на обычный прокси"
    }
  },
